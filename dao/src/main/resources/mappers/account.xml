<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wang.ismy.ssm.dao.AccountDao">

    <select id="findAll" resultType="wang.ismy.ssm.entity.Account">
        SELECT * FROM account;
    </select>
    <select id="findById" resultType="wang.ismy.ssm.entity.Account">
        SELECT * FROM account WHERE id = #{id}
    </select>
</mapper>